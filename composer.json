{
    "name": "somnambulist/value-object-doctrine-mappings",
    "type": "library",
    "description": "A collection of mapping files for the value-objects included in: somnambulist/value-objects",
    "keywords": [
        "doctrine",
        "doctrine mapping",
        "value-objects",
        "ddd"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Redfern",
            "email": "dave@scorpioframework.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Somnambulist\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somnambulist\\Tests\\Doctrine\\": "tests/",
            "Somnambulist\\Tests\\": "tests/_data"
        }
    },
    "require": {
        "php": ">7",
        "doctrine/dbal": "^2.5",
        "somnambulist/doctrine-enum-bridge": "1.*",
        "somnambulist/value-objects": "1.1.*"
    },
    "require-dev": {
        "doctrine/orm": "2.5.*",
        "phpunit/phpunit": "6.2.*",
        "symfony/var-dumper": "^3.3",
        "symfony/yaml": "^3.3",
        "ramsey/uuid": "3.6.*"
    }
}
